{
	"name": "kyc-server",
	"version": "0.1.0",
	"description": "Kyc server using Bun, React, and TailwindCSS",
	"private": true,
	"type": "module",
	"main": "src/index.tsx",
	"module": "src/index.tsx",
	"scripts": {
		"dev": "cross-env NODE_ENV=development bun src/index.tsx",
		"build": "bun build src/index.tsx --outdir dist",
		"start": "cross-env NODE_ENV=production bun src/index.tsx",
		"lint": "eslint . --ext .ts,.tsx",
		"format": "prettier --write \"**/*.{ts,tsx}\"",
		"db:generate": "drizzle-kit generate:pg",
		"db:push": "drizzle-kit push:pg",
		"test": "bun test src/__tests__/*.test.ts",
		"test:watch": "bun test --watch src/__tests__/*.test.ts"
	},
	"dependencies": {
		"@dnd-kit/core": "^6.3.1",
		"@dnd-kit/modifiers": "^9.0.0",
		"@dnd-kit/sortable": "^10.0.0",
		"@dnd-kit/utilities": "^3.2.2",
		"@hookform/resolvers": "^5.1.1",
		"@radix-ui/react-checkbox": "^1.3.2",
		"@radix-ui/react-dialog": "^1.1.14",
		"@radix-ui/react-dropdown-menu": "^2.1.15",
		"@radix-ui/react-label": "^2.1.7",
		"@radix-ui/react-select": "^2.2.5",
		"@radix-ui/react-separator": "^1.1.7",
		"@radix-ui/react-slot": "^1.2.3",
		"@radix-ui/react-tabs": "^1.1.12",
		"@radix-ui/react-toggle": "^1.1.9",
		"@radix-ui/react-toggle-group": "^1.1.10",
		"@simplewebauthn/server": "^13.1.1",
		"@supabase/supabase-js": "^2.39.0",
		"@tanstack/react-table": "^8.21.3",
		"base64url": "^3.0.1",
		"bun-plugin-tailwind": "^0.0.14",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"lucide-react": "^0.513.0",
		"postgres": "^3.4.0",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-hook-form": "^7.57.0",
		"react-router-dom": "^7.2.0",
		"react-use": "^17.6.0",
		"recharts": "^2.15.3",
		"tailwind-merge": "^3.3.0",
		"tailwindcss": "^3.4.1",
		"tailwindcss-animate": "^1.0.7",
		"tw-animate-css": "^1.3.4",
		"vaul": "^1.1.2",
		"ws": "^8.16.0",
		"zod": "^3.25.62",
		"@packages/lib": "workspace:*",
		"@packages/drizzle": "workspace:*"
	},
	"devDependencies": {
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"@types/bun": "latest",
		"@types/ws": "^8.5.10",
		"cross-env": "^7.0.3",
		"drizzle-kit": "^0.20.14",
		"eslint": "^8.56.0",
		"prettier": "^3.1.0",
		"typescript": "^5.3.0"
	}
}
