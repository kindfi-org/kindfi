{
	"name": "kyc-server",
	"version": "0.1.0",
	"description": "An express application using Apollo for server client queries and PostgreSQL with Drizzle ORM.",
	"main": "src/app.ts",
	"scripts": {
		"start": "ts-node src/app.ts",
		"test": "bun test",
		"type-check": "bun eslint",
		"type-check:fix": "bun eslint --fix",
