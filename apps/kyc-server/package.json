	},
	"devDependencies": {
		"@types/express": "^5.0.0",
		"@types/pg": "^8.11.10",
		"drizzle-kit": "^0.30.1",
		"ts-node": "^10.4.0",
		"ts-node-dev": "^1.1.8",
		"tsx": "^4.19.2",
		"typescript": "^4.5.4"
	},
