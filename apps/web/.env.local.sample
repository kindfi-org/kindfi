# Local development example (.env.local)

# App environment
NEXT_PUBLIC_APP_ENV=development

# App
APP_NAME="web"

# Contracts / chain IDs (non-secret, adjust if needed)
ACCOUNT_SECP256K1_CONTRACT_WASM="<account-secp256k1-contract-wasm-hash>"
CONTROLLER_ACCOUNT_SECP256K1_CONTRACT_WASM="<controller-secp256k1-contract-wasm-hash>"
CONTROLLER_CONTRACT_ID="<controller-contract-id>"
FACTORY_CONTRACT_ID="<factory-contract-id>"

# Local Supabase (local development instance)
NEXT_PUBLIC_SUPABASE_URL="http://127.0.0.1:54321"
NEXT_PUBLIC_SUPABASE_ANON_KEY="<supabase-anon-key-local>"
SUPABASE_SERVICE_ROLE_KEY="<supabase-service-role-key-local>"
SUPABASE_JWT_SECRET="<supabase-jwt-secret-local>"

# Local app & APIs
NEXT_PUBLIC_APP_URL="http://localhost:3000"
KYC_API_BASE_URL="http://localhost:3001"
NEXT_PUBLIC_KYC_API_BASE_URL="http://localhost:3001"
NEXTAUTH_SECRET="<nextauth-secret-local>"

# Local DB URL (example)
SUPABASE_DB_URL="postgresql://postgres:postgres@127.0.0.1:54322/postgres"

# Local Trusless Work / platform settings
NEXT_PUBLIC_ROLE_ADDRESS="<role-address>"
NEXT_PUBLIC_PLATFORM_FEE=2
NEXT_PUBLIC_TRUSTLESS_WORK_API_KEY="<trustless-work-api-key-local>"

# Optional: Stellar signature rate limiting (example)
STELLAR_SIGNATURE_MAX_ATTEMPTS=5
STELLAR_SIGNATURE_WINDOW_MS=60000
