# Development 

# App environment
NEXT_PUBLIC_APP_ENV=development

# App
APP_NAME="web"

# Analytics & web config
NEXT_PUBLIC_GA_MEASUREMENT_ID="<G-XXXXXXXXXX>"

# Horizon / Stellar (futurenet / dev RPC)
NEXT_PUBLIC_HORIZON_URL="https://horizon-futurenet.stellar.org"
NEXT_PUBLIC_STELLAR_NETWORK_URL="https://rpc-futurenet.stellar.org"

# Contracts / chain IDs (dev)
ACCOUNT_SECP256K1_CONTRACT_WASM="<account-secp256k1-contract-wasm-hash>"
CONTROLLER_ACCOUNT_SECP256K1_CONTRACT_WASM="<controller-secp256k1-contract-wasm-hash>"
CONTROLLER_CONTRACT_ID="<controller-contract-id>"
FACTORY_CONTRACT_ID="<factory-contract-id>"

# Push / VAPID (mail / push)
NEXT_PUBLIC_VAPID_PUBLIC_KEY="<vapid-public-key>"
VAPID_PRIVATE_KEY="<vapid-private-key>"
VAPID_EMAIL="your-email@example.com"

# Email provider / Resend
RESEND_SMTP_API_KEY="<resend-smtp-api-key>"

# Upstash Redis (staging)
UPSTASH_REDIS_REST_URL="<https://...upstash.io>"
UPSTASH_REDIS_REST_TOKEN="<upstash-token>"

# Vercel / CI auth (example placeholder)
VERCEL_OIDC_TOKEN="<vercel-oidc-token>"

# Supabase dev keys (uncomment and fill for staging if used)
# NEXT_PUBLIC_SUPABASE_ANON_KEY="<supabase-anon-key-dev>"
# SUPABASE_SERVICE_ROLE_KEY="<supabase-service-role-key-dev>"
# NEXT_PUBLIC_SUPABASE_URL="<https://your-dev-supabase-url>"
# SUPABASE_JWT_SECRET="<supabase-jwt-secret-dev>"

# Dev app & APIs
NEXT_PUBLIC_APP_URL="https://dev.kindfi.org"
KYC_API_BASE_URL="https://dev-api.dashboard.kindfi.org"
NEXT_PUBLIC_KYC_API_BASE_URL="https://dev-api.dashboard.kindfi.org"
NEXTAUTH_SECRET="<nextauth-secret-dev>"

# Trustless Work (staging)
NEXT_PUBLIC_ROLE_ADDRESS="<role-address>"
NEXT_PUBLIC_PLATFORM_FEE=2
NEXT_PUBLIC_TRUSTLESS_WORK_API_KEY="<trustless-work-api-key-dev>"

# Optional: Stellar signature rate limiting
STELLAR_SIGNATURE_MAX_ATTEMPTS=5
STELLAR_SIGNATURE_WINDOW_MS=60000
