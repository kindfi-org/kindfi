{
  "model": "age-gender",
  "version": "1.0",
  "description": "Model for predicting age and gender from images.",
  "input": {
    "type": "image",
    "format": "RGB",
    "size": "224x224"
  },
  "output": {
    "type": "json",
    "fields": [
      {
        "name": "age",
        "type": "integer",
        "description": "Predicted age of the individual."
      },
      {
        "name": "gender",
        "type": "string",
        "description": "Predicted gender of the individual (male/female)."
      }
    ]
  },
